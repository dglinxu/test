    "s=input()\n",
    "def f(s):\n",
    "    t=[-1]\n",
    "    r=[]\n",
    "    for i in s:\n",
    "        d=int(i)\n",
    "        r.append(d)\n",
    "        if d>t[-1]:\n",
    "            t.extend([i for i in range(d-1) if i not in r])\n",
    "        elif d==t[-1]:\n",
    "            t.pop()            \n",
    "        else:\n",
    "            return 'No'\n",
    "    return 'Yes'\n",
    "print(f(s))"
